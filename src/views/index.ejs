    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="../CSS/style.css">
    <title><%=title%></title>
   
</head>
<body style="background-color:black;">
  <!-- Start Of Navbar -->
  <nav class="navbar navbar-expand-md navbar-dark fixed-top" style="background-color: rgb(131, 8, 8);">
  <a class="navbar-brand" href="/" style="margin-left: 100px; color: rgb(255, 255, 255); font-weight: bolder;font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;">HOME</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#collapsibleNavbar">
  <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="collapsibleNavbar">
    <ul class="navbar-nav">
      <% for(i=0;i<nav.length;i++){%>
        <li class="navitem">
          <a class="nav-link" href="<%=nav[i].link%>" style="font-weight: bolder;color: rgb(255, 255, 255);font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;"><%=nav[i].name%></a>
        </li>
      <%}%>
    </ul>
  </div>  
  </nav>
  <!-- End Of Navbar -->
  
  <!-- Start of upperbar -->
  <div id="library">  
    <div class="container">
    <div class="row">
      <h1 style="font-family:fantasy;"><%=title%></h1>
    </div> 
    </div>
    <!-- End of sidebar -->

    <!-- Start of overlapping segment -->
    <div class="container">
    <div class="row">
    <div class="col-sm-12" style="background-color: rgb(0, 0, 0);">
    <div class="overlap" style="background-color:rgb(70, 3, 3);"></div>
    </div>
    </div> 
    </div>
    <!-- End of overlapping segment -->
  </div>
  <!-- end of upperbar -->

  <!-- Start of overlapping container -->
  <div class="container">
  <div class="row">
  <div class="col-sm-12"> 
  <div class="panel" style="background-color: rgb(0, 0, 0)">
    <!-- here panel is used to create a bordered box -->
  <div>
    <!-- Start of Articles-->
    <br>
    <br>
    <%for(i=0;i<articles.length;i++){%>
    <div class="row">    
    <br>
    <div class="col-sm-2 text-center">
    </div>
    <div class="col-sm-10">
    <p style="font-weight: bolder;color: rgb(255, 245, 245);font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;"><%=articles[i].title%></p>
    <div class="row">
    <div class="col-xs-9">
    
  
    <small><a href="" style="color: black; text-decoration: none;"></a></small></h4>
    </div>
    </div>
    <br><br>
    </div>
    </div>
    <hr>
    <%}%>
    <!-- End of Articles-->

   
  </div>
</div>
</div>
</div>
</div>
</div>

<br>
<br>
<br>
<br>
<br>
<br>
<!-- Start of footer -->
<div class="container">
<nav class="navbar fixed-bottom navbar-dark" style="background-color:rgb(51, 10, 3);height:6%; width:100%;">
</nav>
</div>
<!-- End of footer -->
</body>
</html>